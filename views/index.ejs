<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link
      rel="stylesheet"
      href="/stylesheets/style.css"
    />
  </head>
  <body>
    <h1><%= title %></h1>
    <% messages.forEach(function(message) { %>
    <p><%= message.text %></p>
    <p><%= message.user %></p>
    <p><%= message.added.fromNow() %></p>

    <% }); %>
    <a href="/new"><button>Click to Send a message</button></a>
  </body>
</html>
